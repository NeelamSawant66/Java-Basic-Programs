import java.util.Scanner;

public class tablesTill {
    public static void main(String[] args) {
        System.out.println("Enter  number till tables you want");
        Scanner tb = new Scanner(System.in);
        int n = tb.nextInt();

        for (int i = 0; i <=n; i++) {
            for (int j = 0; j<=10; j++) {
                int ans = i*j;
                System.out.print( ans+" " );
            }
            System.out.println("");
        }

    }
}
